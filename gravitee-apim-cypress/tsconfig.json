{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "dom"],
    "types": ["cypress", "node", "jsonwebtoken"],
    "baseUrl": "./cypress",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@fakers/*": ["fixtures/fakers/*"],
      "@keys/*": ["fixtures/keys/*"],
      "@model/*": ["model/*"],
      "@commands/*": ["commands/*"]
    }
  },
  "include": ["**/*.ts"]
}
